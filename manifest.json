{
	"manifest_version": 2,
	
	"name": "Env Swap",
	"description": "Easily transfer between development environments",
	"version": "3.1.1",
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"64": "icons/icon64.png",
		"128": "icons/icon128.png",
		"256": "icons/icon256.png",
		"512": "icons/icon512.png"
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"storage"
	],
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	}
}
